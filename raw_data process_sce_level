colnames(Cellratio)[1] <- "Cell_type"
allcolour=c("#244552","#25706B","#28968B","#88AC79","#E1BF6A","#EB9D62","#DA6C4F","#1E90FF","#7CFC00","#FFFF00",
            "#808000","#FF00FF","#FA8072","#7B68EE","#9400D3","#800080","#A0522D","#D2B48C","#D2691E","#87CEEB","#40E0D0","#5F9EA0",
